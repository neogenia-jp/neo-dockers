<?xml version='1.0' encoding='UTF-8'?>
<hudson>
  <disabledAdministrativeMonitors/>
  <version>1.0</version>
  <numExecutors>2</numExecutors>
  <mode>NORMAL</mode>
  <useSecurity>true</useSecurity>
  <authorizationStrategy class="hudson.security.GlobalMatrixAuthorizationStrategy">
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Create:neogenia-jenkins</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Delete:neogenia-jenkins</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.ManageDomains:neogenia-jenkins</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Update:neogenia-jenkins</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.View:neogenia-jenkins</permission>
    <permission>hudson.model.Computer.Build:neogenia-jenkins</permission>
    <permission>hudson.model.Computer.Configure:neogenia-jenkins</permission>
    <permission>hudson.model.Computer.Connect:neogenia-jenkins</permission>
    <permission>hudson.model.Computer.Create:neogenia-jenkins</permission>
    <permission>hudson.model.Computer.Delete:neogenia-jenkins</permission>
    <permission>hudson.model.Computer.Disconnect:neogenia-jenkins</permission>
    <permission>hudson.model.Hudson.Administer:neogenia-jenkins</permission>
    <permission>hudson.model.Hudson.ConfigureUpdateCenter:neogenia-jenkins</permission>
    <permission>hudson.model.Hudson.Read:neogenia-jenkins</permission>
    <permission>hudson.model.Hudson.Read:remote-builder</permission>
    <permission>hudson.model.Hudson.RunScripts:neogenia-jenkins</permission>
    <permission>hudson.model.Hudson.UploadPlugins:neogenia-jenkins</permission>
    <permission>hudson.model.Item.Build:neogenia-jenkins</permission>
    <permission>hudson.model.Item.Build:remote-builder</permission>
    <permission>hudson.model.Item.Cancel:neogenia-jenkins</permission>
    <permission>hudson.model.Item.Cancel:remote-builder</permission>
    <permission>hudson.model.Item.Configure:neogenia-jenkins</permission>
    <permission>hudson.model.Item.Create:neogenia-jenkins</permission>
    <permission>hudson.model.Item.Delete:neogenia-jenkins</permission>
    <permission>hudson.model.Item.Discover:neogenia-jenkins</permission>
    <permission>hudson.model.Item.Discover:remote-builder</permission>
    <permission>hudson.model.Item.Read:neogenia-jenkins</permission>
    <permission>hudson.model.Item.Read:remote-builder</permission>
    <permission>hudson.model.Item.Workspace:neogenia-jenkins</permission>
    <permission>hudson.model.Run.Delete:neogenia-jenkins</permission>
    <permission>hudson.model.Run.Update:neogenia-jenkins</permission>
    <permission>hudson.model.View.Configure:neogenia-jenkins</permission>
    <permission>hudson.model.View.Create:neogenia-jenkins</permission>
    <permission>hudson.model.View.Delete:neogenia-jenkins</permission>
    <permission>hudson.model.View.Read:neogenia-jenkins</permission>
    <permission>hudson.scm.SCM.Tag:neogenia-jenkins</permission>
  </authorizationStrategy>
  <securityRealm class="hudson.security.HudsonPrivateSecurityRealm">
    <disableSignup>true</disableSignup>
    <enableCaptcha>false</enableCaptcha>
  </securityRealm>
  <disableRememberMe>false</disableRememberMe>
  <projectNamingStrategy class="jenkins.model.ProjectNamingStrategy$DefaultProjectNamingStrategy"/>
  <workspaceDir>${ITEM_ROOTDIR}/workspace</workspaceDir>
  <buildsDir>${ITEM_ROOTDIR}/builds</buildsDir>
  <markupFormatter class="hudson.markup.EscapedMarkupFormatter"/>
  <jdks/>
  <viewsTabBar class="hudson.views.DefaultViewsTabBar"/>
  <myViewsTabBar class="hudson.views.DefaultMyViewsTabBar"/>
  <clouds/>
  <scmCheckoutRetryCount>0</scmCheckoutRetryCount>
  <views>
    <hudson.model.AllView>
      <owner class="hudson" reference="../../.."/>
      <name>All</name>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
    </hudson.model.AllView>
  </views>
  <primaryView>All</primaryView>
  <slaveAgentPort>0</slaveAgentPort>
  <label></label>
  <nodeProperties/>
  <globalNodeProperties/>
</hudson>